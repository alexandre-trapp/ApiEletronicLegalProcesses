@{
    ViewData["Title"] = "Integração Jurídico";
}

<div class="row" style="width: 50%">
    <div>
        <h4 style="margin-top: 30px">Processos</h4>

        <table id="tabelaProcessos" class="table table-responsive table-hover">

            <tr>
                <th class="col-lg-3">
                    Data de distribuição
                </th>
                <th class="col-sm-10">
                    Número do processo
                </th>
            </tr>

            <tr style="cursor: pointer">
                <td class="col-lg-3">
                    20/03/2019
                </td>
                <td class="col-sm-10">
                    0599794-97.1991.8.02.0008
                </td>
            </tr>

            <tr style="cursor: pointer">
                <td class="col-lg-3">
                    22/03/2019
                </td>
                <td class="col-sm-10">
                    1782102-62.1994.8.05.0892
                </td>
            </tr>
        </table>

        <form id="formularioProcesso">
            <div class="container">
                <h3>Processo selecionado</h3>
                <hr>

                <label><b>Dados básicos</b></label><p>
                    <textarea class="texto" type="text" readonly="readonly" id="dadosBasicos" required></textarea>
                <p></p>

                <label><b>Movimentações</b></label>
                <p>
                    <textarea class="texto" type="text" readonly="readonly" id="movimentacoes" required></textarea>
            </div>
        </form>

    </div>

</div>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/js/VisualizarDadosProcesso.js"></script>
